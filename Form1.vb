                My.Settings.persian_title = "گروه نرم افزاری اتم"
            Else
                My.Settings.persian_title = TextBox_first_enter_in_app_step_4_TextBox_app_name_persian.Text
            End If
            If TextBox_first_enter_in_app_step_4_TextBox_app_name_english.Text = String.Empty Then
                My.Settings.english_title = "ATOM Software Group"
            Else
                My.Settings.english_title = TextBox_first_enter_in_app_step_4_TextBox_app_name_english.Text
            End If

            PictureBox_first_enter_in_app_Picture_next.Visible = False

            enter_app()
            Radio_persian.Checked = True
            persian_language()
            Label_title.Text = My.Settings.persian_title

            My.Settings.first_enter_in_app = 0

            Me.Cursor = Cursors.Default
        End If






        If PictureBox_first_enter_in_app_step_3_Picture_emoji.Visible = True Then
            If Not TextBox_first_enter_in_app_step_3_TextBox_password_1.Text = String.Empty Then
                If TextBox_first_enter_in_app_step_3_TextBox_password_1.Text = TextBox_first_enter_in_app_step_3_TextBox_password_2.Text Then
                    PictureBox_first_enter_in_app_step_3_Picture_emoji.Visible = False
                    Label_first_enter_in_app_step_3_Label_text.Visible = False
