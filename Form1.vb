                If Label_Accounting_choose_years_4.Text >= 1399 Then Label_Accounting_choose_years_4.Visible = True
                If Label_Accounting_choose_years_6.Text >= 1410 Then Label_Accounting_choose_years_6.Visible = True
                If Label_Accounting_choose_years_7.Text >= 1410 Then Label_Accounting_choose_years_7.Visible = True
                If Label_Accounting_choose_years_8.Text >= 1410 Then Label_Accounting_choose_years_8.Visible = True
                If Label_Accounting_choose_years_9.Text >= 1410 Then Label_Accounting_choose_years_9.Visible = True

                If Label_Accounting_choose_years_1.Text < 1399 Then Label_Accounting_choose_years_1.Visible = False
                If Label_Accounting_choose_years_2.Text < 1399 Then Label_Accounting_choose_years_2.Visible = False
                If Label_Accounting_choose_years_3.Text < 1399 Then Label_Accounting_choose_years_3.Visible = False
                If Label_Accounting_choose_years_4.Text < 1399 Then Label_Accounting_choose_years_4.Visible = False
